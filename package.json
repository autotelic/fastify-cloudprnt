{
  "name": "fastify-plugin-template",
  "version": "0.0.0",
  "description": "Plugin for fastify",
  "main": "index.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "fastify start -w -l info -P -o index.js",
    "example:basic": "fastify start -w -l info -P -o examples/basic/index.js",
    "test": "c8 --100 ava",
    "lint": "standard",
    "fix": "standard --fix",
    "validate": "npm run lint && npm run test",
    "postinstall": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/autotelic/fastify-plugin-template.git"
  },
  "keywords": [],
  "author": "Autotelic Development Ltd <info@autotelic.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/autotelic/fastify-plugin-template/issues"
  },
  "homepage": "https://github.com/autotelic/fastify-plugin-template#readme",
  "dependencies": {
    "@rauschma/stringio": "^1.4.0",
    "execa": "^6.1.0",
    "fastify-plugin": "^3.0.0",
    "nunjucks": "^3.2.3",
    "point-of-view": "^5.2.0"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "c8": "^7.10.0",
    "fastify": "^4.2.0",
    "fastify-cli": "^2.15.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.1.2",
    "node-cache": "^5.1.2",
    "standard": "^17.0.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run fix"
    ]
  }
}
